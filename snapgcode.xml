<blocks app="Snap! 4.0, http://snap.berkeley.edu" version="1">
    <block-definition s="go to x: %&apos;x&apos; y: %&apos;y&apos;"
                      type="command" category="other">
        <inputs>
            <input type="%n">0</input>
            <input type="%n">0</input>
        </inputs>
        <script>
            <block s="gotoXY">
                <block var="x"/>
                <block var="y"/>
            </block>
            <block s="doReport">
                <block s="reportURL">
                    <block s="reportJoinWords">
                        <list>
                            <l>localhost:1234/setxy</l>
                            <block var="x"/>
                            <l>a</l>
                            <block var="y"/>
                        </list>
                    </block>
                </block>
            </block>
        </script>
    </block-definition>
